# msm8953-system-setup service: runs init.qemu-adb-keys.sh script
type msm8953_system_setup, domain, coredomain;
type msm8953_system_setup_exec, system_file_type, exec_type, file_type;

init_daemon_domain(msm8953_system_setup)

allow msm8953_system_setup shell_exec:file { rx_file_perms };

# Allow write to /dev/kmsg
allow msm8953_system_setup kmsg_device:chr_file rw_file_perms;

# Allow read /data/misc/adb/adb_keys
allow msm8953_system_setup adb_keys_file:file r_file_perms;
allow msm8953_system_setup adb_keys_file:dir search;

# Set qemu.adb.copykey
allow msm8953_system_setup toolbox_exec:file { getattr execute read open execute_no_trans map };
set_prop(msm8953_system_setup, vendor_qemu_adb_prop);
